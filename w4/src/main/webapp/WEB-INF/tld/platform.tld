		<name>jsFile</name>
		<function-class>com.rootrip.platform.common.web.el.PlatformFunction</function-class>
		<function-signature>
			java.lang.String jsFile(java.lang.String)
		</function-signature>
		<example>${platform:jsFile(jsFileName)}</example>
	</function>
	
	<function>
		<description></description>
		<name>cssFile</name>
		<function-class>com.rootrip.platform.common.web.el.PlatformFunction</function-class>
		<function-signature>
			java.lang.String cssFile(java.lang.String)
		</function-signature>
		<example>${platform:cssFile(cssFileName)}</example>
	</function>
	
	<function>
		<description></description>
		<name>getBindResultKey</name>
		<function-class>com.rootrip.platform.common.web.el.PlatformFunction</function-class>
		<function-signature>
			java.lang.String getBindResultKey(java.lang.String)
		</function-signature>
		<example>${platform:getBindResultKey(modelAttribute)}</example>
	</function>
	
	<function>
		<description></description>
		<name>replaceImageUploadPath</name>
		<function-class>com.rootrip.platform.common.web.el.PlatformFunction</function-class>
