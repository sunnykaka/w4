		<description></description>
		<name>contains</name>
		<function-class>com.rootrip.platform.common.web.el.PlatformFunction</function-class>
		<function-signature>
			boolean contains(java.lang.Object[], java.lang.Object)
		</function-signature>
		<example>${platform:contains(arr, obj)}</example>
	</function>
	
	<function>
		<description></description>
		<name>colContains</name>
		<function-class>com.rootrip.platform.common.web.el.PlatformFunction</function-class>
		<function-signature>
			boolean colContains(java.util.Collection, java.lang.Object)
		</function-signature>
		<example>${platform:colContains(collection, obj)}</example>
	</function>
	
	<function>
		<description></description>
		<name>params</name>
		<function-class>com.rootrip.platform.common.web.el.PlatformFunction</function-class>
		<function-signature>
